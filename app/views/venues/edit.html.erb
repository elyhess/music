<%= form_tag "/venues/#{@venue.id}", method: :patch do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %><br>

  <%= label_tag :capacity %>
  <%= text_field_tag :capacity %><br>

  <%= label_tag :outdoor %>
  <%= hidden_field_tag :outdoor, ''%>
  <%= check_box_tag :outdoor, true %><br>
  <%= submit_tag 'Submit' %>
<% end %>